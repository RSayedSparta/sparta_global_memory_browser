<Window x:Class="WPFBrowserApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFBrowserApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="540" Width="946">
    <Grid Margin="0,0,-6.6,-4.6">
        <ComboBox x:Name="myComboBox" Text="{Binding urlTxt}" HorizontalAlignment="Left" Margin="25,10,0,0" VerticalAlignment="Top" Width="774" IsEditable="True" SelectionChanged="ComboBox_SelectionChanged" KeyDown="OnKeyPress" SelectedIndex="0">
        </ComboBox>
        <WebBrowser x:Name="Browser" HorizontalAlignment="Left" Height="468" Margin="10,37,0,0" VerticalAlignment="Top" Width="921"/>

    </Grid>
</Window>
